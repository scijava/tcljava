test_tclblend: $(BINARIES) tests/tclblend
	(  echo \
	  set auto_path [concat [pwd] [set auto_path]]\; \
	  cd \{$(FULL_SRCDIR_NATIVE)/tests\}\; \
	  puts \"pwd is [pwd]\"\; \
	  puts \"CLASSPATH is [set env\(CLASSPATH\)]\"\; \
	  puts \"auto_path is [set auto_path]\"\; \
	  source all\; \
	  puts \"Tests done.\"\; \
          exit; ) | \
