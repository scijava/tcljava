    proc clocals_test44 {} {
        upvar #0 UNDEF lvar
        testvarframe
    }
    clocals_tjc_compiled_proc clocals_test44
    clocals_test44
} {localTable is {
UNDEFINED-> lvar -> ::UNDEF
}
compiledLocals is null
compiledLocals.resolved is null
}

